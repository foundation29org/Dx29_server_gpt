# Configuración del servidor
NODE_ENV=development
PORT=8443

# URLs
CLIENT_SERVER=http://localhost:4200

# Base de datos
MONGODBKEY=your_mongodb_key
MONGODBNAME=your_mongodb_name

# Email
EMAIL_PASSWORD=your_email_password

# Azure Storage
OPENDXBLOBKEY=your_blob_key
OPENDXBLOBNAME=your_blob_name

# Azure Service Bus
SERVICE_BUS_NAME=your_service_bus_namespace
SERVICE_BUS_KEY=your_service_bus_key
QUEUE_UTILIZATION_THRESHOLD=80

# API y Seguridad
API_MANAGEMENT_KEY=your_api_management_key
API_MANAGEMENT_BASE=your_api_management_url

INSIGHTS_KEY=your_insights_key
SERVER_KEY=your_server_key

# Servicio de Traducción
TRANSLATION_KEY=your_translation_key
TRANSLATION_KEY_GLOBAL=your_translation_key_global

# Azure AI Studio Configuration
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_KEY=your-azure-openai-key
AZURE_AI_ASSISTANT_ID=asst_your-assistant-id
AZURE_VECTOR_STORE_ID=vs_your-vector-store-id